                 alt="<%= products5[0].name %>"
                 style="height: 45vh; object-fit: cover;"
              />

              <div class="block1-wrapbtn" style="width: 80%">
                <!-- Button -->
                <a
                  href="/product/<%= products5[0].variants[0].slug%>"
                  class="flex-c-m bg3 hov1 trans-0-4"
                >
                  <%=  products5[0].name %>
                </a>
              </div>
            </div>
           <%}%>

            <!-- block1 -->
           <% if(products5[1] && products5[1].variants[0]){%>
