              <% if(p.variants[0]){%>
                <div class="col-sm-12 col-md-6 col-lg-4 p-b-50" id="container">
                <!-- Block2 -->
                <div class="block2">
                  <div
                    class="
                      block2-img
                      wrap-pic-w
                      of-hidden
                      pos-relative
                      block2-labelnew
                    "
