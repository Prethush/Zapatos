                  class="flex-c-m bg3 hov1 trans-0-4"
                >
                  <%=  products5[2].name %>
                </a>
              </div>
            </div>
            <%}%>

            <!-- block1 -->
            
              <% if(products5[3] && products5[3].variants[0]){%>
                <div class="block1 hov-img-zoom pos-relative m-b-30">
              <img
                src="/admin/products/<%=products5[3] ? products5[3].image[0].img : ''%>"
                 alt="<%= products5[3].name %>"
                  style="height: 45vh; object-fit: cover;"
              />

                <div class="block1-wrapbtn" style="width: 80%">
                <!-- Button -->
                <a
                  href="/product/<%=products5[3].variants[0].slug%>"
                  class="flex-c-m bg3 hov1 trans-0-4"
                >
                  <%= products5[3].name %>
                </a>
              </div>
            </div>
              <%}%>
